{
  "name": "heneke/http-serializer-laravel",
  "description": "HTTP message serializer Laravel integration",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Hendrik Heneke",
      "email": "hendrik@pixelwunder.net"
    }
  ],
  "minimum-stability": "stable",
  "repositories": [
    {
      "type": "vcs",
      "url": "ssh://git@github.com/heneke/php-http-serializer"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "heneke/http-serializer": "^0.3.1",
    "illuminate/support": "^5.2",
    "illuminate/routing": "^5.2",
    "symfony/psr-http-message-bridge": "^0.2.0",
    "zendframework/zend-diactoros": "^1.3"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "Heneke\\Http\\Serializer\\Illuminate\\": "src/main/php"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Heneke\\Http\\Serializer\\Illuminate\\": "src/test/php"
    }
  }
}
